{
    "name": "sovic/pays-cz-php-api",
    "description": "Pays.cz PHP SDK for payments REST API",
    "keywords": [
        "pays.cz",
        "payments",
        "php",
        "rest",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
